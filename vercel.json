{
  "version": 2,
  "github": {
    "enabled": false
  },
  "routes": [
    { "src": "^/currently-reading\\.json$", "dest": "/api/currently_reading" },
    { "src": "^/recent-commits\\.json$", "dest": "/api/recent_commits" },
    { "src": "^/recent-tracks\\.json$", "dest": "/api/recent_tracks" },
    { "handle": "filesystem" },
    { "src": "^/\\.well-known(?:/.*)?$" },
    { "src": "^/((?:[^/]+/)*[^/\\.]+)$", "status": 308, "headers": { "Location": "/$1/" } }
  ]
}
